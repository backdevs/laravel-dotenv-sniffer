{
    "name": "backdevs/dotenv-sniffer",
    "type": "library",
    "description": "A code sniffer for environment variables not defined in a .env file.",
    "homepage": "https://github.com/backdevs/php-dotenv-sniffer",
    "license": "MIT",
    "authors": [
        {
            "name": "Bogdan",
            "email": "bogdan@backdevs.net"
        }
    ],
    "keywords": [
        "php",
        "dotenv",
        "sniffer"
    ],
    "bin": [
        "bin/desniff"
    ],
    "require": {
        "php": "^8.1",
        "ext-tokenizer": "*",
        "symfony/console": "^6.2",
        "symfony/validator": "^6.2",
        "vlucas/phpdotenv": "^5.0",
        "symfony/stopwatch": "^6.2"
    },
    "require-dev": {
        "ext-intl": "*",
        "phpunit/phpunit": "^10.0",
        "symfony/var-dumper": "^6.2",
        "squizlabs/php_codesniffer": "^3.7",
        "phpspec/prophecy": "^1.3",
        "phpspec/prophecy-phpunit": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Backdevs\\DotenvSniffer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
